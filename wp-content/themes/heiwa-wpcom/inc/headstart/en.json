            "post_title": "Home",
            "post_content": "<!-- wp:group {\"layout\":{\"inherit\":false},\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"7vw\",\"bottom\":\"7vw\"},\"margin\":{\"top\":\"0px\",\"bottom\":\"0px\"}},\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|foreground\"}}}},\"backgroundColor\":\"background\",\"textColor\":\"foreground\"} -->\n<div class=\"wp-block-group alignfull has-foreground-color has-background-background-color has-text-color has-background has-link-color\" style=\"margin-top:0;margin-bottom:0;padding-top:7vw;padding-bottom:7vw;\"><!-- wp:group {\"layout\":{\"type\":\"flex\",\"orientation\":\"vertical\"}} -->\n<div class=\"wp-block-group\"><!-- wp:group {\"layout\":{\"type\":\"constrained\",\"contentSize\":\"1164px\"}} -->\n<div class=\"wp-block-group\"><!-- wp:heading {\"level\":1} -->\n<h1>Heiwa is a branding and design agency with a passion for simplicity. We deliver high-quality creative solutions for a diverse audience.<\/h1>\n<!-- \/wp:heading --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"margin\":{\"top\":\"0px\",\"bottom\":\"0px\"},\"padding\":{\"top\":\"7vw\",\"bottom\":\"0vw\"}}},\"backgroundColor\":\"background\",\"textColor\":\"foreground\"} -->\n<div class=\"wp-block-group alignfull has-foreground-color has-background-background-color has-text-color has-background\" style=\"margin-top:0;margin-bottom:0;padding-top:7vw;padding-bottom:0;\"><!-- wp:heading {\"fontSize\":\"huge\"} -->\n<h2 class=\"has-huge-font-size\">Our Work<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"0px\",\"left\":\"0px\"}},\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|foreground\"}}}}} -->\n<div class=\"wp-block-group has-link-color\"><!-- wp:columns {\"style\":{\"spacing\":{\"blockGap\":\"56px\"}}} -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:image {\"id\":19,\"sizeSlug\":\"full\",\"linkDestination\":\"custom\",\"className\":\"margin-bottom-half\"} -->\n<figure class=\"wp-block-image size-full margin-bottom-half\"><a href=\"#\"><img src=\"https:\/\/heiwademo.files.wordpress.com\/2022\/05\/water-journal-2bxqtxojhko-unsplash.jpg\" alt=\"\" class=\"wp-image-19\" \/><\/a><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"0px\",\"left\":\"0px\"},\"margin\":{\"top\":\"15px\"}}}} -->\n<div class=\"wp-block-group\" style=\"margin-top:15px;\"><!-- wp:paragraph -->\n<p><a href=\"#\">Brice<\/a><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>2021<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:image {\"id\":21,\"sizeSlug\":\"full\",\"linkDestination\":\"custom\",\"className\":\"margin-bottom-half\"} -->\n<figure class=\"wp-block-image size-full margin-bottom-half\"><a href=\"#\"><img src=\"https:\/\/heiwademo.files.wordpress.com\/2022\/05\/charlesdeluvio-vfq0y6zvex0-unsplash.jpg\" alt=\"\" class=\"wp-image-21\" \/><\/a><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"0px\",\"left\":\"0px\"},\"margin\":{\"top\":\"15px\"}}}} -->\n<div class=\"wp-block-group\" style=\"margin-top:15px;\"><!-- wp:paragraph -->\n<p><a href=\"#\">Vesta Magazine<\/a><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>2021<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns -->\n\n<!-- wp:spacer {\"height\":\"3.5vw\"} -->\n<div style=\"height:3.5vw;\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:columns {\"style\":{\"spacing\":{\"blockGap\":\"56px\"}}} -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:image {\"id\":22,\"sizeSlug\":\"full\",\"linkDestination\":\"custom\",\"className\":\"margin-bottom-half\"} -->\n<figure class=\"wp-block-image size-full margin-bottom-half\"><a href=\"#\"><img src=\"https:\/\/heiwademo.files.wordpress.com\/2022\/05\/water-journal-evvsegvoue-unsplash.jpg\" alt=\"\" class=\"wp-image-22\" \/><\/a><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"0px\",\"left\":\"0px\"},\"margin\":{\"top\":\"15px\"}}}} -->\n<div class=\"wp-block-group\" style=\"margin-top:15px;\"><!-- wp:paragraph -->\n<p><a href=\"#\">Easley Magazine<\/a><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>2021<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:image {\"id\":23,\"sizeSlug\":\"full\",\"linkDestination\":\"custom\",\"className\":\"margin-bottom-half\"} -->\n<figure class=\"wp-block-image size-full margin-bottom-half\"><a href=\"#\"><img src=\"https:\/\/heiwademo.files.wordpress.com\/2022\/05\/sarah-dorweiler-sy8dsvyipgs-unsplash.jpg\" alt=\"\" class=\"wp-image-23\" \/><\/a><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"0px\",\"left\":\"0px\"},\"margin\":{\"top\":\"15px\"}}}} -->\n<div class=\"wp-block-group\" style=\"margin-top:15px;\"><!-- wp:paragraph -->\n<p><a href=\"#\">Overton<\/a><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>2021<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns -->\n\n<!-- wp:spacer {\"height\":\"3.5vw\"} -->\n<div style=\"height:3.5vw;\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"layout\":{\"inherit\":false},\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"7vw\",\"bottom\":\"7vw\"},\"margin\":{\"top\":\"0px\",\"bottom\":\"0px\"}}},\"backgroundColor\":\"background\",\"textColor\":\"foreground\"} -->\n<div class=\"wp-block-group alignfull has-foreground-color has-background-background-color has-text-color has-background\" style=\"margin-top:0;margin-bottom:0;padding-top:7vw;padding-bottom:7vw;\"><!-- wp:heading {\"textAlign\":\"left\",\"fontSize\":\"huge\"} -->\n<h2 class=\"has-text-align-left has-huge-font-size\">Our Process<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:spacer {\"height\":\"1px\"} -->\n<div style=\"height:1px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph {\"align\":\"left\",\"style\":{\"typography\":{\"lineHeight\":\"0.8\",\"fontSize\":\"54px\"}}} -->\n<p class=\"has-text-align-left\" style=\"font-size:54px;line-height:0.8;\">1.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"10px\",\"left\":\"10px\"},\"margin\":{\"top\":\"10px\",\"bottom\":\"0px\"}}}} -->\n<div class=\"wp-block-group\" style=\"margin-top:10px;margin-bottom:0;\"><!-- wp:separator {\"style\":{\"color\":{\"background\":\"#a6a6a6\"}},\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator has-text-color has-alpha-channel-opacity has-background is-style-wide\" style=\"background-color:#a6a6a6;color:#a6a6a6;\" \/>\n<!-- \/wp:separator -->\n\n<!-- wp:heading {\"level\":3} -->\n<h3>Understand<\/h3>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph -->\n<p>Always remember in the jungle there\u2019s a lot of them in there, after you overcome them, you will make it to paradise.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph {\"align\":\"left\",\"style\":{\"typography\":{\"lineHeight\":\"0.8\",\"fontSize\":\"54px\"}}} -->\n<p class=\"has-text-align-left\" style=\"font-size:54px;line-height:0.8;\">2.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"10px\",\"left\":\"10px\"},\"margin\":{\"top\":\"10px\",\"bottom\":\"0px\"}}}} -->\n<div class=\"wp-block-group\" style=\"margin-top:10px;margin-bottom:0;\"><!-- wp:separator {\"style\":{\"color\":{\"background\":\"#a6a6a6\"}},\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator has-text-color has-alpha-channel-opacity has-background is-style-wide\" style=\"background-color:#a6a6a6;color:#a6a6a6;\" \/>\n<!-- \/wp:separator -->\n\n<!-- wp:heading {\"level\":3} -->\n<h3>Imagine<\/h3>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph -->\n<p>You see the hedges, how I got it shaped up? It\u2019s important to shape up your hedges, it\u2019s like getting a haircut, stay fresh.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph {\"align\":\"left\",\"style\":{\"typography\":{\"lineHeight\":\"0.8\",\"fontSize\":\"54px\"}}} -->\n<p class=\"has-text-align-left\" style=\"font-size:54px;line-height:0.8;\">3.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"10px\",\"left\":\"10px\"},\"margin\":{\"top\":\"10px\",\"bottom\":\"0px\"}}}} -->\n<div class=\"wp-block-group\" style=\"margin-top:10px;margin-bottom:0;\"><!-- wp:separator {\"style\":{\"color\":{\"background\":\"#a6a6a6\"}},\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator has-text-color has-alpha-channel-opacity has-background is-style-wide\" style=\"background-color:#a6a6a6;color:#a6a6a6;\" \/>\n<!-- \/wp:separator -->\n\n<!-- wp:heading {\"level\":3} -->\n<h3>Create<\/h3>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph -->\n<p>We don\u2019t see them, we will never see them. To be successful you\u2019ve got to work hard, to make history, simple, you\u2019ve got to make it.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph {\"align\":\"left\",\"style\":{\"typography\":{\"lineHeight\":\"0.8\",\"fontSize\":\"54px\"}}} -->\n<p class=\"has-text-align-left\" style=\"font-size:54px;line-height:0.8;\">4.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":{\"top\":\"10px\",\"left\":\"10px\"},\"margin\":{\"top\":\"10px\",\"bottom\":\"0px\"}}}} -->\n<div class=\"wp-block-group\" style=\"margin-top:10px;margin-bottom:0;\"><!-- wp:separator {\"style\":{\"color\":{\"background\":\"#a6a6a6\"}},\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator has-text-color has-alpha-channel-opacity has-background is-style-wide\" style=\"background-color:#a6a6a6;color:#a6a6a6;\" \/>\n<!-- \/wp:separator -->\n\n<!-- wp:heading {\"level\":3} -->\n<h3>Build<\/h3>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph -->\n<p>Look at the sunset. Surround yourself with angels, positive energy, beautiful people, beautiful souls, clean heart, angel.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"layout\":{\"inherit\":true,\"type\":\"constrained\"},\"align\":\"full\",\"style\":{\"spacing\":{\"margin\":{\"top\":\"0px\",\"bottom\":\"0px\"},\"padding\":{\"top\":\"7vw\",\"bottom\":\"7vw\"}}},\"backgroundColor\":\"tertiary\",\"textColor\":\"foreground\"} -->\n<div class=\"wp-block-group alignfull has-foreground-color has-tertiary-background-color has-text-color has-background\" style=\"margin-top:0;margin-bottom:0;padding-top:7vw;padding-bottom:7vw;\"><!-- wp:paragraph {\"align\":\"left\",\"style\":{\"typography\":{\"lineHeight\":1.5}},\"fontSize\":\"huge\"} -->\n<p class=\"has-text-align-left has-huge-font-size\" style=\"line-height:1.5;\">\u201cHeiwa was amazing from start to finish. we couldn\u2019t believe the incredibly innovative approach and the efficient client service all the way.\u201d<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph {\"align\":\"left\"} -->\n<p class=\"has-text-align-left\">\u2014 Marcus Dimitri , True Snow<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"layout\":{\"inherit\":false},\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"14vw\",\"bottom\":\"14vw\"},\"margin\":{\"top\":\"0px\",\"bottom\":\"0px\"}}},\"backgroundColor\":\"background\",\"textColor\":\"foreground\"} -->\n<div class=\"wp-block-group alignfull has-foreground-color has-background-background-color has-text-color has-background\" style=\"margin-top:0;margin-bottom:0;padding-top:14vw;padding-bottom:14vw;\"><!-- wp:heading {\"textAlign\":\"center\",\"style\":{\"typography\":{\"lineHeight\":\"1.1\",\"fontSize\":\"54px\"}}} -->\n<h2 class=\"has-text-align-center\" style=\"font-size:54px;line-height:1.1;\">Let's work together.<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:buttons {\"layout\":{\"type\":\"flex\",\"justifyContent\":\"center\"}} -->\n<div class=\"wp-block-buttons\"><!-- wp:button {\"backgroundColor\":\"foreground\",\"textColor\":\"background\",\"style\":{\"border\":{\"radius\":\"0px\"}}} -->\n<div class=\"wp-block-button\"><a class=\"wp-block-button__link has-background-color has-foreground-background-color has-text-color has-background wp-element-button\" style=\"border-radius:0;\">Contact Us<\/a><\/div>\n<!-- \/wp:button --><\/div>\n<!-- \/wp:buttons --><\/div>\n<!-- \/wp:group -->",
